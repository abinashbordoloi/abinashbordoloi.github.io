ress\",\n      addressLocality: contactPageData.addressSection?.locality,\n      addressRegion: contactPageData.addressSection?.region,\n      addressCountry: contactPageData.addressSection?.country,\n      postalCode: contactPageData.addressSection?.postalCode,\n      streetAddress: contactPageData.addressSection?.streetAddress,\n    },\n    hasCredential: credentials,\n  };\n  return (\n    <Helmet>\n      <title>{seo.title}</title>\n      <meta name=\"description\" content={seo.description} />\n      <meta property=\"og:title\" content={seo?.og?.title} />\n      <meta property=\"og:type\" content={seo?.og?.type} />\n      <meta property=\"og:url\" content={seo?.og?.url} />\n      <script type=\"application/ld+json\">{JSON.stringify(data)}</script>\n    </Helmet>\n  );\n}\n\nexport default SeoHeader;\n"],"mappings":"AAAA,MAAO,CAAAA,KAAK,KAAM,OAAO,CACzB,OAASC,MAAM,KAAQ,cAAc,CACrC,OACEC,QAAQ,CACRC,GAAG,CACHC,gBAAgB,CAChBC,UAAU,CACVC,eAAe,CACfC,cAAc,KACT,oBAAoB,CAE3B,QAAS,CAAAC,SAASA,CAAA,CAAG,KAAAC,oBAAA,CAAAC,qBAAA,CAAAC,sBAAA,CAAAC,OAAA,CAAAC,qBAAA,CAAAC,qBAAA,CAAAC,sBAAA,CAAAC,sBAAA,CAAAC,sBAAA,CAAAC,sBAAA,CAAAC,QAAA,CAAAC,QAAA,CAAAC,QAAA,CACnB,GAAI,CAAAC,MAAM,CAAG,EAAE,CACflB,gBAAgB,CACbmB,MAAM,CACL,SAACC,KAAK,QACJ,EAAEA,KAAK,CAACC,IAAI,CAACC,UAAU,CAAC,KAAK,CAAC,EAAIF,KAAK,CAACC,IAAI,CAACC,UAAU,CAAC,QAAQ,CAAC,CAAC,EACtE,CAAC,CACAC,OAAO,CAAC,SAACH,KAAK,CAAK,CAClBF,MAAM,CAACM,IAAI,CAACJ,KAAK,CAACC,IAAI,CAAC,CACzB,CAAC,CAAC,CAEJ,GAAI,CAAAI,IAAI,CAAGzB,gBAAgB,CACxB0B,IAAI,CAAC,SAACN,KAAK,QAAK,CAAAA,KAAK,CAACC,IAAI,CAACC,UAAU,CAAC,QAAQ,CAAC,GAAC,CAChDD,IAAI,CAACM,SAAS,CAAC,SAAS,CAACC,MAAM,CAAC,CACnC,GAAI,CAAAC,GAAG,EAAAxB,oBAAA,CAAGJ,UAAU,CAAC6B,QAAQ,UAAAzB,oBAAA,kBAAAC,qBAAA,CAAnBD,oBAAA,CACNqB,IAAI,CAAC,SAACK,OAAO,QAAK,CAAAA,OAAO,CAACC,IAAI,GAAC,UAAA1B,qBAAA,kBAAAC,sBAAA,CADzBD,qBAAA,CAEN2B,WAAW,UAAA1B,sBAAA,iBAFLA,sBAAA,CAEO2B,EAAE,CAAC,CAAC,CAAC,CAEtB,GAAI,CAAAC,WAAW,CAAG,EAAE,CACpBhC,cAAc,CAACA,cAAc,CAACoB,OAAO,CAAC,SAACa,aAAa,CAAK,CACvDD,WAAW,CAACX,IAAI,CAAC,CA